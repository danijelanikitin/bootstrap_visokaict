name = Bootstrap Visokaict
description = Tema Visoka ICT realizovana uz pomoc bootstrap-a.
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;
regions[fast_navigation]    = 'Fast navigation'
regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_row_one]       = 'Page row one'
regions[page_row_two]       = 'Page row two'
regions[page_banner]       = 'Page banner'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/socicon.css
stylesheets[all][] = css/style.css
stylesheets[all][] = font-awesome/css/font-awesome.min.css

